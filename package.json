{
  "name": "stack-trace-logger",
  "version": "1.0.6",
  "description": "Custom winston logger to support line-trace logs and build in seq log monitor, aws could-watch logs",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "logger",
    "typescript",
    "node",
    "winston",
    "cloudwatch",
    "seq"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdriel/stack-trace-logger.git"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx,json,md}": "prettier --write .",
    "src/**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "files": [
    "dist"
  ],
  "author": "Hadriel Benjo (https://github.com/hdriel)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hdriel/stack-trace-logger/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "prepare": "husky install",
    "format": "prettier --write .",
    "clean": "rimraf ./dist",
    "yalc:publish": "pnpm run build && yalc publish --private --push && cd demo && npm run yalc:attach"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.835.0",
    "@datalust/winston-seq": "2.0.0",
    "get-root-path": "^3.0.1",
    "pathe": "^2.0.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-cloudwatch": "^6.3.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "husky": "^9.1.7",
    "inject-markdown": "^3.1.4",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "vite": "7.1.5",
    "vite-plugin-dts": "^4.5.4"
  }
}
