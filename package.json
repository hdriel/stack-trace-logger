{
  "name": "traced-logger",
  "version": "1.0.2",
  "description": "Custom winston logger to support line-trace logs and build in seq log monitor, aws could-watch logs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write .",
    "build": "tsc",
    "test": "jest",
    "start": "node dist/index.js",
    "dev": "tsx src/example.ts"
  },
  "keywords": [
    "logger",
    "typescript",
    "node",
    "winston",
    "cloudwatch",
    "seq"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdriel/st-logger.git"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx,json,md}": "prettier --write .",
    "src/**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "files": [
    "dist"
  ],
  "author": "Hadriel Benjo (https://github.com/hdriel)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hdriel/st-logger/issues"
  },
  "homepage": "https://github.com/hdriel/st-logger#readme",
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.835.0",
    "@datalust/winston-seq": "2.0.0",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "inject-markdown": "^3.1.4",
    "lint-staged": "^16.1.2",
    "path": "^0.12.7",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-cloudwatch": "^6.3.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "jest": "^30.0.3",
    "pnpm": "^10.12.3",
    "prettier": "^3.6.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  }
}
